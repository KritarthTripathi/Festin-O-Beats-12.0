<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/Dlayout2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFA500"
    tools:context="com.abesit.amritotsavabesit.CulturalselectActivity">

    <com.google.android.material.navigation.NavigationView
        android:id="@+id/nav_view"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_gravity="start"
        app:headerLayout="@layout/header"
        app:menu="@menu/main_menu" />

    <ScrollView
        android:id="@+id/Sview"
        android:layout_width="match_parent"
        android:background="@drawable/b2"
        android:layout_height="match_parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/cL1"
            android:layout_width="match_parent"
            android:layout_height="150dp">


            <androidx.appcompat.widget.Toolbar
                android:id="@+id/toolbar"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:background="#00FFFFFF"
                android:minHeight="?attr/actionBarSize"
                android:theme="?attr/actionBarTheme"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:navigationIcon="@drawable/menu"
                app:title="Cultural Events"
                app:titleTextColor="#E91E63"/>

            <Button
                android:id="@+id/event1cult"
                android:layout_width="300dp"
                android:layout_height="60dp"
                android:layout_marginTop="8dp"
                android:background="@drawable/cultbuttons"
                android:text="Solo Singing"
                android:textColor="#1F1B1B"
                android:textSize="20sp"
                app:cornerRadius="64dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/toolbar" />

            <Button
                android:id="@+id/event2cult"
                android:layout_width="300dp"
                android:layout_height="60dp"
                android:layout_marginTop="8dp"
                android:background="@drawable/cultbuttons"
                android:text="Duet Singing"
                android:textColor="#1F1B1B"
                android:textSize="20sp"
                app:cornerRadius="64dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/event1cult" />

            <Button
                android:id="@+id/event3cult"
                android:layout_width="300dp"
                android:layout_height="60dp"
                android:layout_marginTop="8dp"
                android:background="@drawable/cultbuttons"
                android:text="Group Singing"
                android:textColor="#1F1B1B"
                android:textSize="20sp"
                app:cornerRadius="64dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/event2cult" />

            <Button
                android:id="@+id/event4cult"
                android:layout_width="300dp"
                android:layout_height="60dp"
                android:layout_marginTop="8dp"
                android:background="@drawable/cultbuttons"
                android:text="Solo Dancing"
                android:textColor="#1F1B1B"
                android:textSize="20sp"
                app:cornerRadius="64dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/event3cult" />

            <Button
                android:id="@+id/event5cult"
                android:layout_width="300dp"
                android:layout_height="60dp"
                android:layout_marginTop="8dp"
                android:background="@drawable/cultbuttons"
                android:text="Duet Dancing"
                android:textColor="#1F1B1B"
                android:textSize="20sp"
                app:cornerRadius="64dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/event4cult" />

            <Button
                android:id="@+id/event6cult"
                android:layout_width="300dp"
                android:layout_height="60dp"
                android:layout_marginTop="8dp"
                android:background="@drawable/cultbuttons"
                android:text="Group Dance"
                android:textColor="#1F1B1B"
                android:textSize="20sp"
                app:cornerRadius="64dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/event5cult" />

            <Button
                android:id="@+id/event7cult"
                android:layout_width="300dp"
                android:layout_height="60dp"
                android:layout_marginTop="8dp"
                android:background="@drawable/cultbuttons"
                android:text="Follow The Beats"
                android:textColor="#1F1B1B"
                android:textSize="20sp"
                app:cornerRadius="64dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/event6cult" />

            <Button
                android:id="@+id/event8cult"
                android:layout_width="300dp"
                android:layout_height="60dp"
                android:layout_marginTop="8dp"
                android:background="@drawable/cultbuttons"
                android:text="Hindi Skit"
                android:textColor="#1F1B1B"
                android:textSize="20sp"
                app:cornerRadius="64dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/event7cult" />

            <Button
                android:id="@+id/event9cult"
                android:layout_width="300dp"
                android:layout_height="60dp"
                android:layout_marginTop="8dp"
                android:background="@drawable/cultbuttons"
                android:text="Stand Up Comedy"
                android:textColor="#1F1B1B"
                android:textSize="20sp"
                app:cornerRadius="64dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/event8cult" />

            <Button
                android:id="@+id/event10cult"
                android:layout_width="300dp"
                android:layout_height="60dp"
                android:layout_marginTop="8dp"
                android:background="@drawable/cultbuttons"
                android:text="Fashion Show"
                android:textColor="#1F1B1B"
                android:textSize="20sp"
                app:cornerRadius="64dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/event9cult" />
            <Button
                android:id="@+id/event11cult"
                android:layout_width="300dp"
                android:layout_height="60dp"
                android:layout_marginTop="8dp"
                android:background="@drawable/cultbuttons"
                android:text="Sufi Bands"
                android:textColor="#1F1B1B"
                android:textSize="20sp"
                app:cornerRadius="64dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/event10cult" />

            <LinearLayout
                android:id="@+id/cult_cordi_names"
                android:layout_width="0dp"
                android:layout_height="120dp"
                android:orientation="horizontal"
                android:padding="8dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/event11cult">

                <androidx.cardview.widget.CardView
                    android:id="@+id/cord_1_cult"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_margin="10dp"
                    android:layout_weight="1"
                    app:cardBackgroundColor="#0000">


                    <TextView
                        android:id="@+id/info1_cult1"
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:layout_gravity="bottom|center_horizontal"
                        android:fontFamily="@font/baloo"
                        android:gravity="center_horizontal|top|center_vertical"
                        android:text="Ayush Mishra"
                        android:textColor="@color/black" />

                    <TextView
                        android:id="@+id/contact_cult1"
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:layout_gravity="bottom"
                        android:fontFamily="@font/baloo"
                        android:gravity="center_horizontal|center_vertical"
                        android:text="+91 9354007718"
                        android:textIsSelectable="true"
                        android:textColor="@color/black"
                        android:textSize="10sp"
                        app:layout_constraintTop_toBottomOf="@id/info1_cult1" />


                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/cord_2_cult"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_margin="10dp"
                    android:layout_weight="1"
                    app:cardBackgroundColor="#0000">


                    <TextView
                        android:id="@+id/info1_cult2"
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:layout_gravity="bottom|center_horizontal"
                        android:fontFamily="@font/baloo"
                        android:gravity="center_horizontal|top|center_vertical"
                        android:text="Harshita Phulwani"
                        android:textColor="@color/black" />

                    <TextView
                        android:id="@+id/contact_cult2"
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:layout_gravity="bottom"
                        android:fontFamily="@font/baloo"
                        android:gravity="center_horizontal|center_vertical"
                        android:text="+91 6387891733"
                        android:textIsSelectable="true"
                        android:textColor="@color/black"
                        android:textSize="10sp"
                        app:layout_constraintTop_toBottomOf="@id/info1_cult1" />

                </androidx.cardview.widget.CardView>
            </LinearLayout>


        </androidx.constraintlayout.widget.ConstraintLayout>

    </ScrollView>


</androidx.drawerlayout.widget.DrawerLayout>